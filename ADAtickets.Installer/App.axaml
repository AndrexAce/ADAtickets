<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="ADAtickets.Installer.App"
             RequestedThemeVariant="Default">
    <Application.Styles>
		<FluentTheme>
			<FluentTheme.Palettes>
				<ColorPaletteResources x:Key="Light" Accent="#7A6040" />
				<ColorPaletteResources x:Key="Dark" Accent="#7A6040" />
			</FluentTheme.Palettes>
		</FluentTheme>
    </Application.Styles>
</Application>
